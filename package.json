{
  "name": "Cinemaddict",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Киноман» от HTML Academy",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack",
    "eslint": "eslint src/**/*.js",
    "test": "npm run eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": ["npm run eslint"]
  },
  "devDependencies": {
    "@webpack-cli/init": "0.3.0",
    "eslint": "6.1.0",
    "eslint-config-htmlacademy": "0.5.2",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "moment": "2.25.3",
    "moment-locales-webpack-plugin": "1.2.0",
    "webpack": "^4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "engines": {
    "node": "10"
  }
}
